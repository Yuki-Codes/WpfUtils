<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SolidColorBrush
		x:Key="ForegroundBrush"
		Color="#000000" />

	<SolidColorBrush
		x:Key="ForegroundLightBrush"
		Color="#1A1A1A" />

	<SolidColorBrush
		x:Key="BackgroundBrush"
		Color="#CDCDCD" />

	<SolidColorBrush
		x:Key="BackgroundLightBrush"
		Color="#333333" />

	<SolidColorBrush
		x:Key="TrimBrush"
		Color="DeepPink" />

	<SolidColorBrush
		x:Key="ControlBackgroundBrush"
		Color="#E7E7E7" />

	<SolidColorBrush
		x:Key="ControlHighlightBrush"
		Color="#C0C0C0" />

	<SolidColorBrush
		x:Key="ControlForegroundHighlightBrush"
		Color="#000000" />

	<SolidColorBrush
		x:Key="ControlBackgroundSelectedBrush"
		Color="#A0A0A0" />

</ResourceDictionary>